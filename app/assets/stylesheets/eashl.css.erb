table.clubs {

}
table.clubs tr td:nth-child(1),
table.clubs tr td:nth-child(2) {
	width: 1px !important;
}
table.clubs tr td:nth-child(3) {
	width: inherit !important;
}
table.clubs tr td:nth-child(4),
table.clubs tr td:nth-child(5),
table.clubs tr td:nth-child(6),
table.clubs tr td:nth-child(7),
table.clubs tr td:nth-child(8),
table.clubs tr td:nth-child(9),
table.clubs tr td:nth-child(10),
table.clubs tr td:nth-child(11),
table.clubs tr td:nth-child(12),
table.clubs tr td:nth-child(13){
	width: 1px !important;
	font-size: 11px;
}
table.clubs tr td {
	vertical-align: middle !important;
	padding: 6px 12px !important;
}
table.clubs tr td span {
	display: block;
	font-weight: bold;
	font-size: 16px;
}

table.clubs tr td:nth-child(1),
table.clubs tr td:nth-child(3) {
	font-size: 20px !important;
	font-weight: bold;
}
table.clubs tr td:nth-child(3) {
	text-align: left !important;
}