a.event-register {
	display: inline-block;
	background: #FFAB4D;
	padding: 6px;
	text-decoration: none;
	border-radius: 6px;
}
a.event-register div.event-register-title {
	color: #FFF;
  	text-decoration: none;
  	font-size: 30px;
  	font-weight: 100;
  	text-align: center;
}
a.event-register table.event-register-clock {
	/*background: #FC0;*/
	border-spacing: 4px;
}
a.event-register table.event-register-clock tr td {
	padding: 0;
	background: #090;
}
a.event-register table.event-register-clock tr:last-child td {
	padding: 0;
	background: none;
}
